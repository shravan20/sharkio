{
  "name": "traffic-sniffer",
  "version": "1.0.0",
  "description": "A nodejs proxy that reads all the traffic and makes sense of it",
  "main": "./dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "nodemon main.ts",
    "dev": "nodemon main.ts",
    "prod": "node main.js",
    "docker:build": "docker build . -t sharkio-traffic-sniffer",
    "docker:run": "docker run -p 5012:5012 --name sharkio-sniffer -d sharkio-traffic-sniffer",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"{,!(node_modules)/**/}*.ts\""
  },
  "nodemonConfig": {
    "ignore": [
      "./sniffers-setup.json",
      "./sniffers-setup-temp.json",
      "./local_db.json"
    ]
  },
  "author": "Ido David",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "http-proxy-middleware": "^2.0.6",
    "node-json-db": "^2.2.0",
    "npm": "^9.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "yargs": "^17.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.76",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.29",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "typescript": "^5.0.2"
  }
}
